{
  "name": "management",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js --delay 2",
    "dev": "concurrently --kill=others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "@babel/preset-env": "^7.9.5",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.3",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "concurrently": "^5.1.0"
  }
}
